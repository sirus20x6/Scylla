{
  "name": "quick-scan",
  "description": "Fast scanning with minimal details for quick analysis",
  "version": "1.0",
  "analysis": {
    "enableIATScanning": true,
    "deepIATScan": false,
    "iatScanThreads": 4,
    "maxIATEntries": 5000,
    "resolveImportNames": true,
    "validateImportAddresses": false,
    "rebuildForwarders": false,
    "analyzeSections": false,
    "calculateSectionHashes": false,
    "detectAnomalies": false,
    "scanFullMemory": false,
    "memoryChunkSize": 1048576,
    "scanTimeout": 10,
    "apiResolveTimeout": 5
  },
  "packerDetection": {
    "enableSignatureDetection": true,
    "enableHeuristicDetection": false,
    "enableEntropyAnalysis": false,
    "entropyThreshold": 7.0,
    "minConfidence": 70,
    "suspicionThreshold": 50,
    "signatureDatabasePath": "",
    "autoUpdateSignatures": false,
    "analyzeOverlay": false,
    "analyzeResources": false,
    "detectCustomPackers": false,
    "maxSignaturesToTest": 50,
    "stopOnFirstMatch": true
  },
  "performance": {
    "workerThreads": 4,
    "enableParallelProcessing": true,
    "enableCaching": true,
    "apiCacheSize": 500,
    "peCacheSize": 25,
    "cacheTTL": 30,
    "maxCacheMemoryMB": 100,
    "useMemoryMapping": true,
    "ioBufferSize": 65536,
    "asyncIO": false
  },
  "output": {
    "defaultFormat": "text",
    "verbosity": "minimal",
    "showSections": false,
    "showImports": true,
    "showExports": false,
    "showResources": false,
    "showSecurity": true,
    "showPacker": true,
    "showHashes": false,
    "colorOutput": true,
    "prettyPrint": false,
    "indentSize": 2,
    "outputDirectory": "",
    "timestampFiles": false,
    "overwriteExisting": true
  }
}
